--Mainn
local Library = Update:Window("              XandarHUB","rbxassetid://",Enum.KeyCode.RightControl);
Infor = Library:Tab("เกี่ยวข้อง ฮับ",102152430984661)
Main = Library:Tab("ออโต้ ฟาร์ม ทั่วไป",114177316268156)
Setting = Library:Tab("ตั้งค่า",114177316268156)
